{
	"$schema": "https://turbo.build/schema.json",
	"globalDependencies": ["**/.env.*local"],
	"pipeline": {
	  "build": {
		"dependsOn": ["^build"],
		"outputs": ["dist/**", ".next/**", "!.next/cache/**"],
		"env": []
	  },
	  "lint": {
		"outputs": []
	  },
	  "dev": {
		"cache": false
	  },
	  "start:dev": {
		"cache": false
	  },
	  "test:e2e": {
		"cache": false
	  },
	  "test": {
		"dependsOn": ["^lint", "^test:e2e", "^build"],
		"outputs": ["coverage/**"]
	  }
	}
  }